            <Button Style="{DynamicResource ButtonStyle2}"  Content="Уровень 2"  Height="45" Click="Button_ClickL_LVL2" Width="200" Margin="15" >
            </Button>
            <Button Style="{DynamicResource ButtonStyle2}"  Content="Уровень 3" Click="Button_ClickL_LVL3" Height="45" Width="200" Margin="15">
            </Button>
            <Label Name="losa" Foreground="{DynamicResource TextPrimaryColor}"/>
        </StackPanel>
        <Button Style="{DynamicResource ButtonStyle2}" Content="Счет" Click="chet" Width="76" Height="30" Margin="606,392,38,28">
        </Button>
        <Frame x:Name="PagesNavigation" NavigationUIVisibility="Hidden">

        </Frame>
    
    </Grid>
</Page>
